<!doctype html>
<html class="fixed">
<head>

    <!-- Basic -->
    <meta charset="UTF-8">

    <title>Hidro Dizayn</title>

    <meta name="keywords" content="HTML5 Admin Template" />
    <meta name="description" content="Porto Admin - Responsive HTML5 Template">
    <meta name="author" content="okler.net">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/vendor/animate/animate.compat.css">
    <link rel="stylesheet" href="~/vendor/font-awesome/css/all.min.css" />
    <link rel="stylesheet" href="~/vendor/boxicons/css/boxicons.min.css" />
    <link rel="stylesheet" href="~/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="~/vendor/bootstrap-datepicker/css/bootstrap-datepicker3.css" />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/css/theme.css" />

    <!-- Skin CSS -->
    <link rel="stylesheet" href="~/css/skins/default.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/css/custom.css">

    <!-- Head Libs -->
    <script src="~/vendor/modernizr/modernizr.js"></script>

    <script src="~/vendor/jquery/jquery.js"></script>

</head>
<body>
    <section class="body">

        <!-- start: header -->
        <header class="header">
            <div class="logo-container">
                <a href="../4.1.0" class="logo">
                    <img src="~/img/HidroDizayn_Logo.png" width="275" height="45" alt="Porto Admin" />
                </a>

                <div class="d-md-none toggle-sidebar-left" data-toggle-class="sidebar-left-opened" data-target="html" data-fire-event="sidebar-left-opened">
                    <i class="fas fa-bars" aria-label="Toggle sidebar"></i>
                </div>

            </div>


        </header>
        <!-- end: header -->


        <div class="inner-wrapper">
            <!-- start: sidebar -->
            <aside id="sidebar-left" class="sidebar-left">


                <div class="sidebar-header">
                    <div class="row">
                        <div class="col">
                            <div class="sidebar-toggle d-none d-md-block" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
                                <i class="fas fa-bars" aria-label="Toggle sidebar"></i>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="nano">
                    <div class="nano-content">

                            <ul class="nav nav-main">
                                <li>
                                    <a class="nav-link" href="/Home/index">
                                        <i class="bx bx-home-alt" aria-hidden="true"></i>
                                        <span>Anasayfa</span>
                                    </a>
                                </li>
                                <li class="nav-parent">
                                    <a class="nav-link" href="#">
                                        <i class="fa-solid fa-list" aria-hidden="true"></i>
                                        <span>Kategoriler</span>
                                    </a>
                                    <ul class="nav nav-children">
                                        <li>
                                            <a class="nav-link" href="/Home/ComputerDetailsIndex">
                                                Bilgisayar Bilgileri
                                            </a>
                                        </li>

                                        <li>
                                            <a class="nav-link" href="/Home/monitorIndex">
                                                Monitör
                                            </a>
                                        </li>

                                        <li>
                                            <a class="nav-link" href="/Home/KullanicilarIndex">
                                                Kullanıcılar
                                            </a>
                                        </li>

                                        <li>
                                            <a class="nav-link" href="/Home/departmanIndex">
                                                Departman
                                            </a>
                                        </li>

                                        <li>
                                            <a class="nav-link" href="/Home/DonanimDegismeIndex">
                                                Donanım Değişme
                                            </a>
                                        </li>

                                        <li>
                                            <a class="nav-link" href="/Home/emanetIndex">
                                                Emanet
                                            </a>
                                        </li>
                                        
                                    </ul>
                                </li>

                                <li class="nav-parent">
                                    <a class="nav-link" href="#">
                                        <i class="fa-solid fa-boxes-stacked" aria-hidden="true"></i>
                                        <span>Envanter Bilgisi</span>
                                    </a>
                                    <ul class="nav nav-children">

                                        <li>
                                            <a class="nav-link" href="/Home/envanterLaptopIndex">
                                                Envanter Laptop
                                            </a>
                                        </li>
                                        <li>
                                            <a class="nav-link" href="/Home/envanterMalzemeIndex">
                                                Envanter Malzeme
                                            </a>
                                        </li>
                                    </ul>
                                </li>

                                <li class="nav-parent">
                                    <a class="nav-link" href="#">
                                        <i class="fa-solid fa-arrows-to-dot" aria-hidden="true"></i>
                                        <span>Puanlama Sistemi</span>
                                    </a>
                                    <ul class="nav nav-children">

                                        <li>
                                            <a class="nav-link" href="/Score/PuanlamaSistemiIndex">
                                                Puanlama
                                            </a>
                                        </li>

                                    <li>
                                        <a class="nav-link" href="/Score/SoruIndex">
                                            Soru Gir
                                        </a>
                                    </li>                                   
                                    </ul>
                                </li>

                                <li class="nav-parent">
                                    <a class="nav-link" href="#">
                                        <i class="fa-solid fa-paperclip" aria-hidden="true"></i>
                                        <span>Fotokopi Ve Malzeme Talep Sistemi</span>
                                    </a>
                                    <ul class="nav nav-children">

                                        <li>
                                            <a class="nav-link" href="/Home/FotokopiTalepIndex">
                                                Fotokopi Takip Sistemi
                                            </a>
                                        </li>

                                    <li>
                                        <a class="nav-link" href="/Home/MalzemeTalepIndex">
                                            Fotokopi Takip Sistemi
                                        </a>
                                    </li>

                                    </ul>
                                </li>





                            </ul>
                        






                    </div>

                    <script>
                        // Maintain Scroll Position
                        if (typeof localStorage !== 'undefined') {
                            if (localStorage.getItem('sidebar-left-position') !== null) {
                                var initialPosition = localStorage.getItem('sidebar-left-position'),
                                    sidebarLeft = document.querySelector('#sidebar-left .nano-content');

                                sidebarLeft.scrollTop = initialPosition;
                            }
                        }
                    </script>

                </div>

            </aside>
            <!-- end: sidebar -->

            <section role="main" class="content-body">
                <header class="page-header">
                    <h2>Ana Sayfa</h2>

                </header>
                @RenderBody()
                @RenderSection("Scripts", required: false)

            </section>
        </div>
    </section>



    <!-- Vendor -->

    <script src="~/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="~/vendor/popper/umd/popper.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/vendor/common/common.js"></script>
    <script src="~/vendor/nanoscroller/nanoscroller.js"></script>
    <script src="~/vendor/magnific-popup/jquery.magnific-popup.js"></script>
    <script src="~/vendor/jquery-placeholder/jquery.placeholder.js"></script>

    <!-- Specific Page Vendor -->
    <!-- Theme Base, Components and Settings -->
    <script src="~/js/theme.js"></script>

    <!-- Theme Custom -->
    <script src="~/js/custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="~/js/theme.init.js"></script>

</body>
</html>

@if (TempData["Message"] != null)
{
    <script type="text/javascript">
        window.onload = function () {
            var message = '@TempData["Message"]';
            alert(message);
        };
    </script>
}